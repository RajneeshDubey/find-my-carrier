{
	"name": "@seller-center/icons",
	"version": "0.0.6",
	"description": "React Icon components (TSX) generated from LD3 svg icons",
	"repository": "https://gecgithub01.walmart.com/RET-Marketplace/seller-center-next-gen",
	"author": "ret-sellercenter-eng@email.wal-mart.com",
	"sideEffects": false,
	"license": "UNLICENSED",
	"main": "dist/components/index.js",
	"module": "dist/es6/components/index.js",
	"esnext": "dist/es6/components/index.js",
	"types": "dist/types/components/index.d.ts",
	"scripts": {
		"start": "start-storybook -p 6004 --no-manager-cache",
		"storybook-start": "echo \"Dummy storybook-start - icons\"",
		"storybook-build": "echo \"Dummy storybook-build - icons\"",
		"start-storybook-local": "start-storybook -p 6004 --no-manager-cache",
		"build": "npm run preBuild && npm run build-icons && npm run build-babel && npm run build-babel-es6 && npm run build-types && npm run postBuild",
		"build-babel": "babel build --out-dir ./dist --ignore 'build/iconsOptimized/*.svg' --copy-files --no-copy-ignored --extensions '.tsx'",
		"build-babel-es6": "cross-env BABEL_ENV=es6 babel build --out-dir ./dist/es6 --ignore 'build/iconsOptimized/*.svg' --copy-files --no-copy-ignored --extensions '.tsx'",
		"build-icons": "npm run build-svg && npm run build-keywords",
		"preBuild": "node -r ./tools/ts-node.register.js ./scripts/preBuild.ts",
		"postBuild": "node -r ./tools/ts-node.register.js ./scripts/postBuild.ts",
		"build-svg": "node -r ./tools/ts-node.register.js ./scripts/generateIconComponents.ts",
		"build-keywords": "node -r ./tools/ts-node.register.js ./scripts/generateKeywords.ts",
		"build:dry": "build-storybook --no-dll",
		"build-story": "storybook-to-ghpages --out ../../.built_for_github_pages/icons -t GH_API_TOKEN --ci",
		"dry-deploy": "storybook-to-ghpages --out ../../.built_for_github_pages/icons --dry-run",
		"build-types": "tsc --emitDeclarationOnly"
	},
	"files": [
		"dist"
	],
	"peerDependencies": {
		"react": ">= 16.8.0",
		"react-dom": ">= 16.8.0"
	},
	"devDependencies": {
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"classnames": "~2.2.6",
		"@svgr/cli": "5.4.0",
		"@storybook/react": "6.1.14",
		"tsconfig-paths-webpack-plugin": "~3.3.0",
		"@babel/plugin-proposal-object-rest-spread": "~7.12.1",
		"@babel/preset-react": "~7.12.7",
		"@babel/preset-env": "~7.12.7",
		"@babel/preset-typescript": "~7.12.7",
		"cross-env": "~7.0.3",
		"svgo": "~1.3.2",
		"fs-extra": "~9.0.1",
		"ts-node": "~9.1.0",
		"typescript": "~4.1.2",
		"tslib": "~2.0.3",
		"@babel/types": "~7.12.7",
		"jest": "~26.6.3",
		"@testing-library/react": "~11.2.2",
		"ts-jest": "~26.4.4",
		"@testing-library/jest-dom": "~5.11.6",
		"@types/jest": "~26.0.16",
		"babel-jest": "~26.6.3",
		"@types/react": "~17.0.0",
		"@babel/cli": "~7.12.10",
		"@babel/core": "^7.12.3",
		"@seller-center/toolchain-storybook": "~1.0.0"
	}
}
