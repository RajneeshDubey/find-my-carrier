	"author": "ret-sellercenter-eng@email.wal-mart.com",
	"description": "tailwindcss utils for Seller Center",
	"main": "tailwind.config.js",
	"scripts": {
		"clean": "rm -rf dist/",
		"minify": "node minify.js",
		"tailwind:build": "tailwindcss build tailwind.styles.css -o dist/output.css",
		"tailwind:build-preflight": "tailwindcss build preflight.styles.css -o dist/output-preflight.css",
		"build": "npm run clean && npm run tailwind:build && npm run tailwind:build-preflight && npm run generate-defaults",
		"storybook-start": "echo \"Dummy storybook-start - styles\"",
		"storybook-build": "echo \"Dummy storybook-build - styles\"",
		"generate-defaults": "node generateDefaultsAndMinimise.js"
	},
