		"autoprefixer": "~8.6.5",
		"clean-css": "~4.2.3",
		"cli-msg": "~1.0.0",
		"gzip-size": "~6.0.0",
		"rimraf": "~3.0.2",
		"tailwindcss": "~2.0.1",
		"postcss": "~8.2.1",
		"normalize.css": "~8.0.1",
		"@seller-center/design-tokens": "~0.0.4"
	}
